{
  "version": 2,
  "name": "lens-lounge-frontend",
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "installCommand": "yarn install --immutable",
  "buildCommand": "yarn build",
  "outputDirectory": "dist"
}