{
  "version": 2,
  "name": "lens-lounge-frontend",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "installCommand": "yarn install --immutable",
  "buildCommand": "yarn build",
  "outputDirectory": "dist"
}
