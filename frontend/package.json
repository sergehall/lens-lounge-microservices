{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "packageManager": "yarn@4.9.2",
  "type": "module",
  "dependencies": {
    "@reduxjs/toolkit": "^2.6.0",
    "axios": "^1.7.9",
    "framer-motion": "^12.12.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "react-router-dom": "6.22.3",
    "styled-components": "^6.1.13",
    "uuid": "^11.0.3",
    "zod": "^3.24.2"
  },
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "dev:debug": "vite --debug hmr",
    "build:strict": "yarn lint && yarn check:types && yarn build",
    "start:server": "ts-node src/mocks/mock-server/server.ts",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ci": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "format": "prettier . --write",
    "lint": "eslint src vite.config.ts --ext .js,.ts,.tsx",
    "lint:fix": "eslint src vite.config.ts --ext .js,.ts,.tsx --fix",
    "lint:light": "eslint src --ext .ts,.tsx --no-error-on-unmatched-pattern",
    "check:types": "tsc --noEmit"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/express": "^5.0.2",
    "@types/identity-obj-proxy": "^3",
    "@types/jsdom": "^21.1.7",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4",
    "@types/node": "^22.15.30",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "@types/react-redux": "^7.1.34",
    "@types/styled-components": "^5.1.34",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "@vitejs/plugin-react": "^4.5.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.4.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.5.3",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3",
    "vite": "^6.3.5",
    "vitest": "^3.2.3",
    "whatwg-fetch": "^3.6.20"
  }
}
