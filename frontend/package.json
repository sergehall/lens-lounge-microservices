  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "start:prod": "vite --mode production",
    "dev:debug": "vite --debug hmr",
    "start:server": "ts-nodyarn install --immutable\ne src/mocks/mock-server/server.ts",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:ci": "jest --coverage --runInBand",
    "test:coverage": "jest --cyarn install --immutable\noverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf .yarn/cache .yarn/install-state.* .pnp.* yarn.lock && yarn cache clean && yarn install"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
